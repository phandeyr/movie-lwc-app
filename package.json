{
    "name": "movie-lwc-app",
    "version": "0.0.1",
    "author": "Renuka Phandey",
    "bugs": "https://github.com/phandeyr/movie-lwc-app/issues",
    "dependencies": {
        "@salesforce-ux/design-system": "^2.14.2",
        "compression": "^1.7.4",
        "express": "^4.17.1",
        "helmet": "^4.4.1",
        "lightning-base-components": "^1.11.2-alpha"
    },
    "devDependencies": {
        "eslint": "^7.20.0",
        "gh-pages": "^3.1.0",
        "husky": "^5.0.9",
        "lint-staged": "^10.5.4",
        "lwc-services": "^3.0.4",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.2.1"
    },
    "engines": {
        "node": ">=10.13.0",
        "npm": ">=6.4.1",
        "yarn": ">=1.9.4"
    },
    "homepage": "https://phandeyr.github.io/phandeyr/movie-lwc-app",
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "keywords": [
        "lwc"
    ],
    "license": "MIT",
    "lint-staged": {
        "**/*.{css,html,js,json,md,ts,yaml,yml}": [
            "prettier --write"
        ],
        "./src/**/*.js": [
            "eslint"
        ]
    },
    "nodemonConfig": {
        "watch": [
            "src/server/**/*.js"
        ],
        "ext": "js",
        "ignore": [
            "src/**/*.spec.js",
            "src/**/*.test.js"
        ],
        "exec": "node ./src/server/api.js"
    },
    "repository": "phandeyr/movie-lwc-app",
    "scripts": {
        "build": "lwc-services build -m production",
        "build:development": "lwc-services build",
        "lint": "eslint ./src/**/*.js",
        "prettier": "prettier --write \"**/*.{css,html,js,json,md,ts,yaml,yml}\"",
        "prettier:verify": "prettier --list-different \"**/*.{css,html,js,json,md,ts,yaml,yml}\"",
        "serve": "node src/server/api.js",
        "test:unit": "lwc-services test:unit",
        "test:unit:coverage": "lwc-services test:unit --coverage",
        "test:unit:debug": "lwc-services test:unit --debug",
        "test:unit:watch": "lwc-services test:unit --watch",
        "watch": "lwc-services watch",
        "predeploy": "npm run build",
        "deploy": "gh-pages -d dist"
    }
}
